<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.adonev.hibernateEntityFramework">
    <class name="User" table="user" schema="public">
        <id name="userId">
            <column name="id" />
        </id>
    </class>

    <class name="Account" table="account" schema="public">
        <id name="accountId">
            <column name="account_Id" />
        </id>
    </class>

    <class name="Transfer" table="transfer" schema="public">
        <id name="transferId">
            <column name="transfer_Id" />
        </id>
    </class>

</hibernate-mapping>